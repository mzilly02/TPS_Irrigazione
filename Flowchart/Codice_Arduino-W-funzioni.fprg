    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="librerieUsate" type="Integer" array="False" size=""/>
            <declare name="pSensori" type="Integer" array="True" size="5"/>
            <declare name="pValvole" type="Integer" array="True" size="5"/>
            <declare name="pTastoSET, UP, DOWN, SW" type="Integer" array="False" size=""/>
            <declare name="tAreeMINUTI" type="Real" array="True" size="5"/>
            <declare name="LUN" type="Integer" array="True" size="5"/>
            <declare name="MAR" type="Integer" array="True" size="5"/>
            <declare name="MER" type="Integer" array="True" size="5"/>
            <declare name="GIO" type="Integer" array="True" size="5"/>
            <declare name="VEN" type="Integer" array="True" size="5"/>
            <declare name="SAB" type="Integer" array="True" size="5"/>
            <declare name="DOM" type="Integer" array="True" size="5"/>
            <assign variable="Interrupt" expression="DisplayHomepTastoSET"/>
            <call expression="Setup"/>
            <call expression="Loop"/>
        </body>
    </function>
    <function name="DisplayHome" type="None" variable="">
        <parameters/>
        <body>
            <declare name="tDisp" type="Integer" array="False" size=""/>
            <assign variable="tDisp" expression="0"/>
            <output expression="displayWrite(HOME)" newline="True"/>
            <if expression="tDisp - millis() == 10000">
                <then>
                    <if expression="pTastoSW == 1">
                        <then>
                            <call expression="SetTimeAreeON"/>
                        </then>
                        <else/>
                    </if>
                    <if expression="pTastoSW == 2">
                        <then>
                            <call expression="SetTimeAree"/>
                        </then>
                        <else/>
                    </if>
                    <if expression="pTastoSW == 3">
                        <then>
                            <call expression="SetTimeDispRTC"/>
                        </then>
                        <else/>
                    </if>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="Loop" type="None" variable="">
        <parameters/>
        <body>
            <assign variable="tGiorno" expression="giornoRTC()"/>
            <assign variable="tORA" expression="oraRTC()"/>

                <then>
                    <for variable="i" start="0" end="5" direction="inc" step="1">
                        <if expression="tORA == LUN[i]">
                            <then>
                                <if expression="pSensori[i] == 1">
                                    <then>
                                        <output expression="Pinmode(pValvole[i], HIGH)" newline="True"/>
